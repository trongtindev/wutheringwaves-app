{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "items": {
      "type": "array",
      "items": {
        "properties": {
          "slug": { "type": "string" },
          "name": { "type": "string" },
          "group": { "type": "string" },
          "category": { "type": "string" },
          "description": { "type": "string" },
          "publishedTime": { "type": "string" },
          "modifiedTime": { "type": "string" }
        },
        "required": ["slug", "name", "group", "category", "description"]
      }
    },
    "publishedTime": { "type": "string" },
    "modifiedTime": { "type": "string" }
  },
  "required": ["items", "publishedTime", "modifiedTime"],
  "type": "object"
}
