{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "items": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": { "type": "integer" },
          "slug": { "type": "string" },
          "rarity": { "type": "integer" },
          "category": {
            "type": "string",
            "enum": [
              "Currency",
              "Development Materials",
              "Valuables",
              "Quests",
              "Supplies",
              "Resources",
              "Weapons"
            ]
          },
          "name": { "type": "string" },
          "nameLocalized": { "type": "object" },
          "publishedTime": {
            "type": "string"
          },
          "modifiedTime": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "slug",
          "rarity",
          "category",
          "name",
          "publishedTime",
          "modifiedTime"
        ]
      }
    }
  },
  "required": ["items"],
  "type": "object"
}
